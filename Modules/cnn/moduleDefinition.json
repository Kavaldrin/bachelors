{
  "name": "cnn",
  "methodToCall": "cnn",
  "info": {
    "path": "Modules.cnn.cnn",
    "formats": ["*.bmp", "*.pbm", "*.pgm", "*.ppm", "*.sr", "*.ras", "*.jpeg", "*.jpg", "*.jpe", "*.jp2", "*.tiff", "*.tif", "*.png"],
    "options": [
      {
        "option": "-modelPath",
        "help": "Path to trained CNN model",
        "required": "true"
      },
      {
        "option": "-thresh",
        "help": "Determines threshold (0-1) to accept image as clear.",
        "required": "true"
      },
      {
        "option": "-visualization",
        "help": "Indicates where generated image will be saved"
      }
    ],
    "help": "This module uses convoluted neural network to determine whether image is blurred."
  }
}